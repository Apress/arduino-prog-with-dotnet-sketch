        <TextBlock x:Name="textBlock2" HorizontalAlignment="Left" Margin="10,102,0,0" TextWrapping="Wrap" Text="Potentiometer:" VerticalAlignment="Top" RenderTransformOrigin="-1.593,-0.469"/>
        <TextBlock x:Name="txtThermistor" HorizontalAlignment="Left" Margin="163,52,0,0" TextWrapping="Wrap" Text="-" VerticalAlignment="Top"/>
        <TextBlock x:Name="txtPhoto" HorizontalAlignment="Left" Margin="163,77,0,0" TextWrapping="Wrap" Text="-" VerticalAlignment="Top"/>
        <TextBlock x:Name="txtPot" HorizontalAlignment="Left" Margin="163,102,0,0" TextWrapping="Wrap" Text="-" VerticalAlignment="Top"/>

    </Grid>
</Page>
